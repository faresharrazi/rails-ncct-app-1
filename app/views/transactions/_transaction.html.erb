<div id="<%= dom_id transaction %>">
  <table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Amount</th>
      <th>Category</th>
      <th>Account</th>
    </tr>
  </thead>
  <tbody>
        <tr>
        <td><%= transaction.title %></td>
        <td><%= transaction.amount %></td>
        <td><%= transaction.category %></td>
        <td> <%= transaction.account&.name || "No Account" %></td>
        </tr>
  </tbody>
</table>

</div>
