<p style="color: green"><%= notice %></p>

<% content_for :title, "Accounts" %>

<h1>Accounts</h1>

<div id="accounts">
<table>
    <thead>
      <tr>
        <th>Account</th>
        <th>Balance</th>
      </tr>
    </thead>
    <tbody>
  <% @accounts.each do |account| %>
    <%= render account %>
  <% end %>
  </tbody>
  </table>
</div>

<%= link_to "New account", new_account_path %>
